namespace function_object_test

fn double(x) { x * 2 }
fn add(x, y) { x + y }

fn main() {
    let f = double
    println(f.name)
    println(f.arity)
    println(f(5))

    let g = add
    println(g.name)
    println(g.arity)
    println(g(3, 4))
}

