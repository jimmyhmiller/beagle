namespace round_truncate_test

// Test round and truncate functions

fn main() {
    // Test round with positive numbers
    println("round tests (positive):")
    println("round(1.4):")
    println(round(1.4))
    println("round(1.5):")
    println(round(1.5))
    println("round(1.6):")
    println(round(1.6))
    println("round(2.5):")
    println(round(2.5))

    // Test round with negative numbers
    println("\nround tests (negative):")
    println("round(-1.4):")
    println(round(-1.4))
    println("round(-1.5):")
    println(round(-1.5))
    println("round(-1.6):")
    println(round(-1.6))

    // Test truncate with positive numbers
    println("\ntruncate tests (positive):")
    println("truncate(1.4):")
    println(truncate(1.4))
    println("truncate(1.9):")
    println(truncate(1.9))
    println("truncate(2.0):")
    println(truncate(2.0))

    // Test truncate with negative numbers
    println("\ntruncate tests (negative):")
    println("truncate(-1.4):")
    println(truncate(-1.4))
    println("truncate(-1.9):")
    println(truncate(-1.9))
    println("truncate(-2.0):")
    println(truncate(-2.0))

    // Test with zero
    println("\nTests with zero:")
    println("round(0.0):")
    println(round(0.0))
    println("truncate(0.0):")
    println(truncate(0.0))

    // Test with integers
    println("\nTests with integers:")
    println("round(5.0):")
    println(round(5.0))
    println("truncate(5.0):")
    println(truncate(5.0))

    ""
}

// Expect
