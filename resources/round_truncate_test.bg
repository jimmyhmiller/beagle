// round(1.4):
// 1.0
// round(1.5):
// 2.0
// round(1.6):
// 2.0
// round(2.5):
// 3.0
//
// round tests (negative):
// round(-1.4):
// -1.0
// round(-1.5):
// -2.0
// round(-1.6):
// -2.0
//
// truncate tests (positive):
// truncate(1.4):
// 1
// truncate(1.9):
// 1
// truncate(2.0):
// 2
//
// truncate tests (negative):
// truncate(-1.4):
// -1
// truncate(-1.9):
// -1
// truncate(-2.0):
// -2
//
// Tests with zero:
// round(0.0):
// 0.0
// truncate(0.0):
// 0
//
// Tests with integers:
// round(5.0):
// 5.0
// truncate(5.0):
// 5
