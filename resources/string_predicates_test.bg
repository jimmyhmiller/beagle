// ends-with? tests:
// 'hello world' ends with 'world'?
// true
// 'hello world' ends with 'hello'?
// false
// 'hello world' ends with 'hello world'?
// true
// 'hello world' ends with ''?
// true
//
// contains? tests:
// 'hello world' contains 'lo wo'?
// true
// 'hello world' contains 'hello'?
// true
// 'hello world' contains 'world'?
// true
// 'hello world' contains 'xyz'?
// false
// 'hello world' contains ''?
// true
//
// Tests with empty string:
// '' starts with ''?
// true
// '' ends with ''?
// true
// '' contains ''?
// true
// '' starts with 'x'?
// false
