namespace nested_closure_bug2

// Test that closures capture variables from main scope correctly
// Expected output:
//   a=100 b=200

fn main() {
    let a = 100

    let level1 = fn() {
        let b = 200

        let level2 = fn() {
            println("a=${a} b=${b}")
        }

        level2()
    }

    level1()
}
