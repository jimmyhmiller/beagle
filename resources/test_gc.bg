namespace test_gc


struct TreeNode {
    left
    right
}



fn create_garbage() {
    let x = TreeNode {
        left: 1,
        right: 2
    }
    println("After first allocate")

    let y = TreeNode {
        left: 3,
        right: 4
    }

    println("Garbage created")

    let z = TreeNode {
        left: 5,
        right: 6
    }
    z
}

fn main() {
    create_garbage()
    println("Garbage created")
    gc();
    println("Garbage collected")
}

// Expect
// After first allocate
// Garbage created
// Garbage created
// Garbage collected
// null