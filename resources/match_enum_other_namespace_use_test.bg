namespace test_color_match

use other_types as other
fn test-color-match() {
    let red = other/make-red()
    let blue = other/make-blue(255)

    match red {
        other/Color.Red => println("Matched Red")
        other/Color.Green => println("Matched Green")
        other/Color.Blue { value: v } => {
            println("Matched Blue:")
            println(v)
        }
    }

    match blue {
        other/Color.Red => println("Matched Red")
        other/Color.Green => println("Matched Green")
        other/Color.Blue { value: v } => {
            println("Matched Blue:")
            println(v)
        }
    }
}

fn main() {
    test-color-match()
    "done"
}

