namespace rust_map_test

fn main() {
    println("Testing Rust PersistentMap with protocols:")

    // Create a map using Rust collections
    let m = {}
    let m = assoc(m, "a", 1)
    let m = assoc(m, "b", 2)
    let m = assoc(m, "c", 3)

    println("Map created:")
    println(format(m, 0))

    println("Length:")
    println(length(m))

    println("Keys:")
    let ks = keys(m)
    println(format(ks, 0))

    println("Get value:")
    println(get(m, "b"))

    ""
}

// Expect
// Testing Rust PersistentMap with protocols:
// Map created:
// {"b": 2, "c": 3, "a": 1}
// Length:
// 3
// Keys:
// ["b", "c", "a"]
// Get value:
// 2
