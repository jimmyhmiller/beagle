namespace mixed_compare_test

fn main() {
    // int < float
    println(1 < 2.5)
    println(3 < 2.5)

    // float < int
    println(1.5 < 3)
    println(3.5 < 2)

    // int > float
    println(3 > 1.5)
    println(1 > 2.5)

    // float > int
    println(2.5 > 1)
    println(0.5 > 2)

    // int <= float
    println(2 <= 2.0)
    println(1 <= 2.5)
    println(3 <= 2.5)

    // float <= int
    println(2.0 <= 2)
    println(1.5 <= 2)
    println(3.5 <= 2)

    // int >= float
    println(2 >= 2.0)
    println(3 >= 2.5)
    println(1 >= 2.5)

    // float >= int
    println(2.0 >= 2)
    println(2.5 >= 1)
    println(0.5 >= 2)

    // int == float (now coerces)
    println(2 == 2.0)
    println(2 == 2.5)

    // float == int
    println(2.0 == 2)
    println(2.5 == 2)
}

