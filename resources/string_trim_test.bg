namespace string-trim-test

fn main() {
    println(trim("  hello  "))
    println(starts-with?("hello world", "hello"))
    println(starts-with?("hello world", "world"))
    println(ends-with?("hello world", "world"))
    println(ends-with?("hello world", "hello"))
}

// @beagle.core.snapshot
// hello
// true
// false
// true
// false
