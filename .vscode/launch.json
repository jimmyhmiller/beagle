{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'main'",
            "cargo": {
                "args": [
                    "build",
                    // "--release",
                    // "--profile=release-with-debug",
                    "--bin=main",
                    "--package=main",
                    "--features=simple-generation,thread-safe,json"
                ],
                "filter": {
                    "name": "main",
                    "kind": "bin"
                }
            },
            "args": ["resources/enum_test.bg"],
            "cwd": "${workspaceFolder}",
            "preRunCommands": [
                "command script import .vscode/rust_prettifier_for_lldb.py"
            ],
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'main'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=main",
                    "--package=main"
                ],
                "filter": {
                    "name": "main",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}",
            "preRunCommands": [
                "command script import .vscode/rust_prettifier_for_lldb.py"
            ],

        }
    ]
}